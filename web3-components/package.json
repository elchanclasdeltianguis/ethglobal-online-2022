{
  "name": "@el-chanclas-del-tianguis/web3-react-components",
  "version": "0.0.5",
  "description": "A library of reusable web3 typescript react components with tailwindcss",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint",
    "clean": "rm -rf dist",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "build": "npm run clean && tsc && cp package.json README.md LICENSE ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elchanclasdeltianguis/ethglobal-online-2022.git"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "react",
    "web3",
    "ethers",
    "ens",
    "tokens",
    "quicknode"
  ],
  "author": "chanclas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elchanclasdeltianguis/ethglobal-online-2022/issues"
  },
  "homepage": "https://github.com/elchanclasdeltianguis/ethglobal-online-2022#readme",
  "dependencies": {
    "@ensdomains/ensjs": "^3.0.0-alpha.20",
    "@types/react": "^18.0.20",
    "ethers": "^5.7.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@mdx-js/react": "^2.1.3",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.11",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.0.3",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.1"
  }
